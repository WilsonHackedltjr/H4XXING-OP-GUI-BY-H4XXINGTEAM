--// Notification System
local NotiHolder = Instance.new("Frame", ScreenGui)
NotiHolder.Size = UDim2.new(0, 300, 1, -50)
NotiHolder.Position = UDim2.new(1, -320, 1, -60)
NotiHolder.BackgroundTransparency = 1

local UIListLayout2 = Instance.new("UIListLayout", NotiHolder)
UIListLayout2.Padding = UDim.new(0, 5)
UIListLayout2.FillDirection = Enum.FillDirection.Vertical
UIListLayout2.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout2.VerticalAlignment = Enum.VerticalAlignment.Bottom

local function Notify(msg)
	local Noti = Instance.new("TextLabel", NotiHolder)
	Noti.Size = UDim2.new(1, 0, 0, 40)
	Noti.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	Noti.TextColor3 = Color3.fromRGB(255, 255, 255)
	Noti.Font = Enum.Font.GothamBold
	Noti.TextSize = 14
	Noti.Text = msg
	Noti.BackgroundTransparency = 0
	Noti.BorderSizePixel = 0
	Instance.new("UICorner", Noti).CornerRadius = UDim.new(0, 8)

	-- Tween In
	Noti.BackgroundTransparency = 1
	Noti.TextTransparency = 1
	game:GetService("TweenService"):Create(Noti, TweenInfo.new(0.3), {BackgroundTransparency = 0, TextTransparency = 0}):Play()

	-- Auto Remove
	task.delay(3, function()
		game:GetService("TweenService"):Create(Noti, TweenInfo.new(0.3), {BackgroundTransparency = 1, TextTransparency = 1}):Play()
		task.wait(0.3)
		Noti:Destroy()
	end)
end

--// RGB Border Effect
task.spawn(function()
	while true do
		for i = 0, 1, 0.01 do
			UIStroke.Color = Color3.fromHSV(i, 1, 1)
			task.wait(0.03)
		end
	end
end)

--// Minimize Behavior
local minimized = false
local fullSize = Frame.Size

MinBtn.MouseButton1Click:Connect(function()
	if minimized then
		Frame:TweenSize(fullSize, "Out", "Quad", 0.3, true)
		for _, v in pairs(ButtonHolder:GetChildren()) do
			if v:IsA("GuiObject") then v.Visible = true end
		end
		Counter.Visible = true
		minimized = false
	else
		Frame:TweenSize(UDim2.new(0, 300, 0, 40), "Out", "Quad", 0.3, true)
		for _, v in pairs(ButtonHolder:GetChildren()) do
			if v:IsA("GuiObject") then v.Visible = false end
		end
		Counter.Visible = false
		minimized = true
	end
end)

--// Close Behavior
CloseBtn.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)

--// Button Functions
RejoinBtn.MouseButton1Click:Connect(function()
	Notify("Rejoining...")
	TeleportService:Teleport(game.PlaceId, player)
end)

local function findSmallestServer()
	Notify("Finding Smallest Server...")
	local servers = {}
	local cursor = ""
	repeat
		local url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100&cursor=%s", game.PlaceId, cursor)
		local response = HttpService:JSONDecode(game:HttpGet(url))
		for _, s in pairs(response.data) do
			if s.playing and s.maxPlayers and s.playing < s.maxPlayers then
				table.insert(servers, s)
			end
		end
		cursor = response.nextPageCursor or ""
	until cursor == "" or not cursor

	table.sort(servers, function(a, b)
		return a.playing < b.playing
	end)

	if #servers > 0 then
		Notify("Hopping to Smallest Server...")
		TeleportService:TeleportToPlaceInstance(game.PlaceId, servers[1].id, player)
	else
		Notify("No Small Server Found!")
	end
end

SmallServerBtn.MouseButton1Click:Connect(findSmallestServer)

NoPlayerBtn.MouseButton1Click:Connect(function()
	Notify("Looking for Empty Server...")
	local cursor = ""
	repeat
		local url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100&cursor=%s", game.PlaceId, cursor)
		local response = HttpService:JSONDecode(game:HttpGet(url))
		for _, s in pairs(response.data) do
			if s.playing == 0 then
				Notify("Hopping to Empty Server...")
				TeleportService:TeleportToPlaceInstance(game.PlaceId, s.id, player)
				return
			end
		end
		cursor = response.nextPageCursor or ""
	until cursor == "" or not cursor
	Notify("No Empty Server Found!")
end)

HighServerBtn.MouseButton1Click:Connect(function()
	Notify("Finding Highest Player Server...")
	local servers = {}
	local cursor = ""
	repeat
		local url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100&cursor=%s", game.PlaceId, cursor)
		local response = HttpService:JSONDecode(game:HttpGet(url))
		for _, s in pairs(response.data) do
			if s.playing and s.maxPlayers and s.playing < s.maxPlayers then
				table.insert(servers, s)
			end
		end
		cursor = response.nextPageCursor or ""
	until cursor == "" or not cursor

	table.sort(servers, function(a, b)
		return a.playing > b.playing
	end)

	if #servers > 0 then
		Notify("Hopping to Highest Player Server...")
		TeleportService:TeleportToPlaceInstance(game.PlaceId, servers[1].id, player)
	else
		Notify("No Server Found!")
	end
end)

--// Player Counter Update
task.spawn(function()
	while task.wait(1) do
		Counter.Text = "Players: " .. tostring(#Players:GetPlayers())
	end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "RBG HEALTHðŸŒˆðŸ’—",
   Callback = function()
   -- The function that takes place when the button is pressed
-- LocalScript (StarterPlayerScripts)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
