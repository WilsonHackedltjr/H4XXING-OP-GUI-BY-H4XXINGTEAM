local Button = Tab:CreateButton({
   Name = "CHECK ANTI-CHEAT STATUSðŸ”ŽðŸš«",
   Callback = function()
   -- The function that takes place when the button is pressed
-- Real Anti-Cheat Status GUI with Notifications & Dragging
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- UI Setup
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "AntiCheatStatusGUI"
gui.ResetOnSpawn = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 300, 0, 150)
frame.Position = UDim2.new(0.3, 0, 0.3, 0)
frame.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
frame.BorderSizePixel = 4
frame.Active = true
frame.Draggable = true -- âœ… Make movable

local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 10)

-- RGB Border Effect
task.spawn(function()
	while frame do
		for h = 0, 1, 0.01 do
			frame.BorderColor3 = Color3.fromHSV(h, 1, 1)
			task.wait(0.02)
		end
	end
end)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0.4, 0)
title.Text = "Anti-Cheat Status:"
title.TextScaled = true
title.Font = Enum.Font.GothamBold
title.BackgroundTransparency = 1
title.TextColor3 = Color3.new(1,1,1)

local statusText = Instance.new("TextLabel", frame)
statusText.Position = UDim2.new(0, 0, 0.4, 0)
statusText.Size = UDim2.new(1, 0, 0.3, 0)
statusText.TextScaled = true
statusText.Font = Enum.Font.GothamBlack
statusText.BackgroundTransparency = 1
statusText.TextColor3 = Color3.new(1, 1, 1)

local close = Instance.new("TextButton", frame)
close.Size = UDim2.new(0, 30, 0, 30)
close.Position = UDim2.new(1, -35, 0, 5)
close.Text = "X"
close.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
