minimizeButton.Size = UDim2.new(0, 20, 0, 20)
minimizeButton.Position = UDim2.new(1, -50, 0, 5)
minimizeButton.Text = "-"
minimizeButton.BackgroundColor3 = Color3.fromRGB(0, 50, 0)
minimizeButton.TextColor3 = Color3.new(0, 1, 0)
minimizeButton.Font = Enum.Font.Code
minimizeButton.TextScaled = true

-- ANIMATE BACKGROUND
task.spawn(function()
	while gui.Parent do
		bg.Text = bg.Text:sub(2) .. (math.random() > 0.5 and "1" or "0")
		task.wait(0.05)
	end
end)

-- MATRIX EFFECT
local function createMatrix()
	local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local folder = workspace:FindFirstChild("MatrixCube") or Instance.new("Folder", workspace)
	folder.Name = "MatrixCube"

	local center = hrp.Position
	local size = 58
	local spacing = 10

	for x = -size/2, size/2, spacing do
		for y = -size/2, size/2, spacing do
			for z = -size/2, size/2, spacing do
				if math.random() < 0.3 then
					local bit = Instance.new("Part")
					bit.Size = Vector3.new(1.5, 1.5, 1.5)
					bit.Anchored = true
					bit.CanCollide = false
					bit.Material = Enum.Material.Neon
					bit.Position = center + Vector3.new(x, y, z)
					bit.Color = math.random() > 0.5 and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)

					local gui = Instance.new("BillboardGui", bit)
					gui.Size = UDim2.new(3, 0, 3, 0)
					gui.AlwaysOnTop = true

					local label = Instance.new("TextLabel", gui)
					label.Size = UDim2.new(1, 0, 1, 0)
					label.BackgroundTransparency = 1
					label.Text = (bit.Color == Color3.fromRGB(0, 255, 0)) and "1" or "0"
					label.TextColor3 = bit.Color
					label.TextScaled = true
					label.Font = Enum.Font.Code

					bit.Parent = folder

					TweenService:Create(bit, TweenInfo.new(2), {
						Position = bit.Position + Vector3.new(0, bit.Color == Color3.fromRGB(0, 255, 0) and 6 or -6, 0),
						Transparency = 1
					}):Play()

					game.Debris:AddItem(bit, 2)
				end
			end
		end
	end
end

-- LOOPING
local loopActive = false
local function startLoop()
	loopActive = true
	task.spawn(function()
		while loopActive do
			createMatrix()
			task.wait(0.15)
		end
	end)
end

local function stopLoop()
	loopActive = false
	local folder = workspace:FindFirstChild("MatrixCube")
	if folder then folder:Destroy() end
end

-- TOGGLE BUTTON
toggleButton.MouseButton1Click:Connect(function()
	if loopActive then
		stopLoop()
		toggleButton.Text = "TOGGLE MATRIX (OFF)"
		toggleButton.TextColor3 = Color3.fromRGB(255, 0, 0)
	else
		startLoop()
		toggleButton.Text = "TOGGLE MATRIX (ON)"
		toggleButton.TextColor3 = Color3.fromRGB(0, 255, 0)
	end
end)

-- CLOSE BUTTON
closeButton.MouseButton1Click:Connect(function()
	stopLoop()
	gui:Destroy()
end)

-- MINIMIZE BUTTON
local minimized = false
minimizeButton.MouseButton1Click:Connect(function()
	minimized = not minimized
	for _, child in pairs(frame:GetChildren()) do
		if child:IsA("TextButton") and child ~= minimizeButton and child ~= closeButton then
			child.Visible = not minimized
		end
	end
	frame.Size = minimized and UDim2.new(0, 240, 0, 40) or UDim2.new(0, 240, 0, 130)
end)
   end,
})

