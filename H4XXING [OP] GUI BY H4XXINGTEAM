closeProfile.TextSize = 14

--// Toggle State
local walking = false
local minimized = false

-- Tween Helpers
local function tweenSize(obj, size)
	TweenService:Create(obj, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Size = size
	}):Play()
end

-- Find Nearest Player
local function getNearestPlayer()
	local char = LocalPlayer.Character
	local hrp = char and char:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local closest, minDist = nil, math.huge
	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local dist = (hrp.Position - player.Character.HumanoidRootPart.Position).Magnitude
			if dist < minDist then
				minDist = dist
				closest = player
			end
		end
	end
	return closest
end

-- Movement Loop
RunService.RenderStepped:Connect(function()
	if walking then
		local nearest = getNearestPlayer()
		local char = LocalPlayer.Character
		local hrp = char and char:FindFirstChild("HumanoidRootPart")
		if nearest and hrp and nearest.Character and nearest.Character:FindFirstChild("HumanoidRootPart") then
			local targetHRP = nearest.Character.HumanoidRootPart
			local direction = (targetHRP.Position - hrp.Position).Unit
			hrp.CFrame = CFrame.new(hrp.Position + direction * 0.5, targetHRP.Position)

			-- Show Profile
			profile.Visible = true
			username.Text = "Username: " .. nearest.Name
			displayName.Text = "DisplayName: " .. nearest.DisplayName
			avatar.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. nearest.UserId .. "&width=150&height=150&format=png"
		else
			profile.Visible = false
		end
	end
end)

--// Button Actions
toggleBtn.MouseButton1Click:Connect(function()
	walking = not walking
	local newColor = walking and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(30, 30, 30)
	TweenService:Create(toggleBtn, TweenInfo.new(0.2), {BackgroundColor3 = newColor}):Play()
end)

minimizeBtn.MouseButton1Click:Connect(function()
	if minimized then
		tweenSize(mainFrame, UDim2.new(0, 300, 0, 160))
		toggleBtn.Visible = true
	else
		toggleBtn.Visible = false
		tweenSize(mainFrame, UDim2.new(0, 300, 0, 30))
	end
	minimized = not minimized
end)

closeBtn.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

closeProfile.MouseButton1Click:Connect(function()
	profile.Visible = false
end)
   end,
})

