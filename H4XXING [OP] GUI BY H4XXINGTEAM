-- FPS ADJUSTER
local currentFPS = 60
local fpsConnection

local function setFPS(value)
	if fpsConnection then fpsConnection:Disconnect() end
	if value >= 1 then
		fpsConnection = RunService.RenderStepped:Connect(function()
			wait(1/value)
		end)
	end
end

