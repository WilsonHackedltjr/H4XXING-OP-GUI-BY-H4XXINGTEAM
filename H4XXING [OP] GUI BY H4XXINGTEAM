-- Animate RGB border
local hue = 0
RunService.RenderStepped:Connect(function()
	hue = (hue + 0.005) % 1
	stroke.Color = Color3.fromHSV(hue, 1, 1)
end)

-- Highlight logic
local highlights = {}
local toggled = false
local minimized = false

local function createHighlight(character)
	if highlights[character] then return end
	local hl = Instance.new("Highlight")
	hl.Name = "RGBOutline"
	hl.FillTransparency = 1
	hl.OutlineTransparency = 0
	hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
	hl.Adornee = character
	hl.Parent = screenGui
	highlights[character] = hl
end

local function removeHighlight(character)
	if highlights[character] then
		highlights[character]:Destroy()
		highlights[character] = nil
	end
end

local function updateAllHighlights()
	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer then
			local char = player.Character
			if char and char:IsDescendantOf(workspace) then
				createHighlight(char)
			end
		end
	end
end

-- Handle new players
Players.PlayerAdded:Connect(function(player)
	player.CharacterAdded:Connect(function(char)
		if toggled then
			task.wait(1)
			createHighlight(char)
		end
	end)
end)

-- Toggle logic
toggle.MouseButton1Click:Connect(function()
	toggled = not toggled
	toggle.BackgroundColor3 = toggled and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(30, 30, 30)

	if toggled then
		updateAllHighlights()
		RunService:BindToRenderStep("HighlightRGB", 301, function()
			local rgb = Color3.fromHSV(tick() % 5 / 5, 1, 1)
			for _, hl in pairs(highlights) do
				if hl and hl.Adornee then
					hl.OutlineColor = rgb
				end
			end
		end)
	else
		for _, hl in pairs(highlights) do
			if hl then hl:Destroy() end
		end
		highlights = {}
		RunService:UnbindFromRenderStep("HighlightRGB")
	end
end)

-- Close Button
close.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

-- Minimize Logic (Rayfield style)
minimize.MouseButton1Click:Connect(function()
	minimized = not minimized
	toggle.Visible = not minimized
	minimize.Text = minimized and "+" or "-"
	frame.Size = minimized and UDim2.new(0, 260, 0, 40) or UDim2.new(0, 260, 0, 120)
end)

-- Initial highlights if already loaded
updateAllHighlights()
   end,
