	if minimized then
		frame:TweenSize(UDim2.new(0, 200, 0, 35), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.25, true)
		toggle.Visible = false
	else
		frame:TweenSize(UDim2.new(0, 200, 0, 90), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.25, true)
		toggle.Visible = true
	end
end)

-- RING FUNCTIONALITY
local orbitConnection
local isOrbiting = false
local radius = 10
local speed = 1.8

local function startOrbit()
	local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	orbitConnection = RunService.Heartbeat:Connect(function(dt)
		hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return end

		local livePlayers = {}
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				local humanoid = plr.Character:FindFirstChildOfClass("Humanoid")
				if humanoid and humanoid.Health > 0 then
					table.insert(livePlayers, plr.Character)
				end
			end
		end

		for i, char in ipairs(livePlayers) do
			local targetHRP = char:FindFirstChild("HumanoidRootPart")
			if targetHRP then
				local angle = (math.pi * 2 / #livePlayers) * i + tick() * speed
				local offset = Vector3.new(math.cos(angle) * radius, 0, math.sin(angle) * radius)
				targetHRP.Anchored = true
				targetHRP.CFrame = CFrame.new(hrp.Position + offset)
			end
		end
	end)
end

local function stopOrbit()
	if orbitConnection then
		orbitConnection:Disconnect()
		orbitConnection = nil
	end

	for _, plr in ipairs(Players:GetPlayers()) do
		if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
			plr.Character.HumanoidRootPart.Anchored = false
		end
	end
end

toggle.MouseButton1Click:Connect(function()
	isOrbiting = not isOrbiting
	toggle.Text = isOrbiting and "STOP RING" or "RING PLAYER CHARACTER"
	if isOrbiting then
		startOrbit()
	else
		stopOrbit()
	end
end)
   end,
})

