profileLabel.Text = "H4XXINGTEAM"
profileLabel.Parent = profileFrame

-- Dragging via topbar
local dragging = false
local dragStart = Vector2.new()
local startPos = UDim2.new()

local function beginDrag(input)
	if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then
		dragging = true
		if input.UserInputType==Enum.UserInputType.Touch then
			dragStart = input.Position
		else
			dragStart = UserInputService:GetMouseLocation()
		end
		startPos = mainFrame.Position
		input.Changed:Connect(function()
			if input.UserInputState==Enum.UserInputState.End then
				dragging=false
			end
		end)
	end
end

topBar.InputBegan:Connect(beginDrag)

UserInputService.InputChanged:Connect(function(input)
	if dragging and (input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch) then
		local currentPos
		if input.UserInputType==Enum.UserInputType.Touch then
			currentPos = input.Position
		else
			currentPos = UserInputService:GetMouseLocation()
		end
		local delta = currentPos - dragStart
		mainFrame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)

-- Minimize & Close
local minimized=false
minimizeBtn.MouseButton1Click:Connect(function()
	minimized = not minimized
	if minimized then
		mainFrame.Size = UDim2.new(0,300,0,30)
		toggleBtn.Visible=false
		profileFrame.Visible=false
	else
		mainFrame.Size = UDim2.new(0,300,0,150)
		toggleBtn.Visible=true
		profileFrame.Visible=true
	end
end)

closeBtn.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

-- RBG Health
local rbgEnabled=false
toggleBtn.MouseButton1Click:Connect(function()
	rbgEnabled = not rbgEnabled
end)

-- Rainbow border & health regen
local hue = 0
RunService.RenderStepped:Connect(function(deltaTime)
	hue = (hue + deltaTime*0.5)%1
	border.Color = Color3.fromHSV(hue,1,1)

	if rbgEnabled and humanoid then
		if not character:FindFirstChild("RBGHealthBar") then
			local billboard = Instance.new("BillboardGui")
			billboard.Name="RBGHealthBar"
			billboard.Adornee=humanoid.RootPart or character.PrimaryPart
			billboard.Size=UDim2.new(4,0,0.5,0)
			billboard.StudsOffset=Vector3.new(0,3,0)
			billboard.AlwaysOnTop=true
			billboard.Parent=character
			local bar = Instance.new("Frame")
			bar.Name="Bar"
			bar.Size=UDim2.new(humanoid.Health/humanoid.MaxHealth,0,1,0)
			bar.BackgroundColor3=Color3.fromHSV(hue,1,1)
			bar.BorderSizePixel=0
			bar.Parent=billboard
		else
			local bar=character.RBGHealthBar:FindFirstChild("Bar")
			if bar then
				bar.Size=UDim2.new(humanoid.Health/humanoid.MaxHealth,0,1,0)
				bar.BackgroundColor3=Color3.fromHSV(hue,1,1)
			end
		end
		humanoid.Health=math.min(humanoid.Health+0.5*deltaTime,humanoid.MaxHealth)
	elseif character:FindFirstChild("RBGHealthBar") then
		character.RBGHealthBar:Destroy()
	end
end)
   end,
})
