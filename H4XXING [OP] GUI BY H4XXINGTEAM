	flag = Instance.new("BoolValue")
	flag.Name = flagName
	flag.Value = false
	flag.Parent = player
end

-- Update toggle UI based on flag
local function updateToggleUI()
	local on = flag.Value
	local targetColor = on and Color3.fromRGB(0, 200, 120) or Color3.fromRGB(60,60,60)
	TweenService:Create(ToggleBtn, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		BackgroundColor3 = targetColor
	}):Play()
	ToggleBtn.Text = "H4X RISE: " .. (on and "ON" or "OFF")
end
updateToggleUI()

-- Make toggle smooth & persistent
ToggleBtn.MouseButton1Click:Connect(function()
	flag.Value = not flag.Value
	updateToggleUI()
end)

-- Close behavior
CloseBtn.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

-- Minimize/Restore behavior (Rayfield-like)
local minimized = false
local busy = false -- prevent double-clicks while animating

local function minimize()
	if busy then return end
	busy = true
	minimized = true
	MinBtn.Text = "+"
	-- shrink content smoothly then shrink main frame
	TweenService:Create(Content, TweenInfo.new(0.22, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Size = UDim2.new(1, 0, 0, 0)
	}):Play()
	local t1 = TweenService:Create(MainFrame, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Size = UDim2.new(0, math.clamp(originalSize.X.Offset * 0.6, 140, 280), 0, 36)
	})
	t1:Play()
	t1.Completed:Wait()
	Content.Visible = false
	busy = false
end

local function restore()
	if busy then return end
	busy = true
	minimized = false
	MinBtn.Text = "â€”"
	Content.Visible = true
	-- restore main frame size first, then expand content
	local t1 = TweenService:Create(MainFrame, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Size = originalSize
	})
	t1:Play()
	t1.Completed:Wait()
	TweenService:Create(Content, TweenInfo.new(0.18, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Size = contentOriginalSize
	}):Play()
	busy = false
end

MinBtn.MouseButton1Click:Connect(function()
	if minimized then
		restore()
	else
		minimize()
	end
end)

-- Dragging via TopBar (works even when minimized)
local dragging = false
local dragStart = Vector2.new(0,0)
local startPos = UDim2.new(0,0,0,0)

TopBar.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = MainFrame.Position
		-- capture end
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
				-- update stored original position when user finishes dragging
				originalPosition = MainFrame.Position
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
		local delta = input.Position - dragStart
		-- Calculate new position using offset arithmetic
		local newX = startPos.X.Offset + delta.X
		local newY = startPos.Y.Offset + delta.Y
		MainFrame.Position = UDim2.new(startPos.X.Scale, newX, startPos.Y.Scale, newY)
	end
end)

-- When screen/gui resizes (optional): keep within screen (small guard)
local function clampToScreen()
	local sg = game:GetService("GuiService")
	-- Keep MainFrame inside screen bounds (soft clamp)
	local screenSize = workspace.CurrentCamera and workspace.CurrentCamera.ViewportSize or Vector2.new(1280,720)
	local width = MainFrame.AbsoluteSize.X
	local height = MainFrame.AbsoluteSize.Y
	local pos = MainFrame.AbsolutePosition
	local x = math.clamp(pos.X, 0, math.max(0, screenSize.X - width))
	local y = math.clamp(pos.Y, 0, math.max(0, screenSize.Y - height))
	MainFrame.Position = UDim2.new(0, x, 0, y)
	originalPosition = MainFrame.Position
end

-- Optional: clamp after a short delay if needed (keeps it usable on resolution change)
delay(1, clampToScreen)

-- Rise effect on respawn
local function onCharacterAdded(char)
	local success, err = pcall(function()
		if not flag.Value then return end
		local hum = char:WaitForChild("Humanoid", 5)
		local root = char:WaitForChild("HumanoidRootPart", 5)
		if not hum or not root then return end

		-- Place a bit below start position
		local startCFrame = root.CFrame
		local below = startCFrame * CFrame.new(0, -6, 0)
		root.CFrame = below

		-- Set health to 1 and animate rise
		hum.Health = 1
		local targetCFrame = startCFrame
		local riseTween = TweenService:Create(root, TweenInfo.new(1.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = targetCFrame})
		riseTween:Play()

		-- smooth health regen over ~1.6-2.0s
		local maxH = hum.MaxHealth or 100
		local regenTime = 1.8
		local steps = math.max(8, math.floor(regenTime / 0.05))
		for i = 1, steps do
			local fraction = i / steps
			pcall(function()
				hum.Health = math.clamp(1 + (maxH - 1) * fraction, 1, maxH)
			end)
			task.wait(regenTime / steps)
		end
		-- ensure full at end
		hum.Health = maxH
	end)
	if not success then
		warn("H4X_Rise onCharacterAdded error:", err)
	end
end

-- Connect character add
player.CharacterAdded:Connect(onCharacterAdded)
-- If character already exists (script ran after spawn)
if player.Character then
	-- small safe spawn
	task.spawn(function() onCharacterAdded(player.Character) end)
end

-- Keep UI consistent with persistent flag if it was toggled before
flag.Changed:Connect(updateToggleUI)
   end,
})

