-- FUNCTIONALITY
local isFacing = false

local function getNearestPlayer()
	local nearest, minDist = nil, math.huge
	for _, other in ipairs(Players:GetPlayers()) do
		if other ~= player and other.Character and other.Character:FindFirstChild("HumanoidRootPart") then
			local dist = (character:FindFirstChild("HumanoidRootPart").Position - other.Character.HumanoidRootPart.Position).Magnitude
			if dist < minDist then
				minDist = dist
				nearest = other
			end
		end
	end
	return nearest
end

toggle.MouseButton1Click:Connect(function()
	isFacing = not isFacing
	toggle.Text = isFacing and "Facing ON" or "Facing OFF"
end)

RunService.RenderStepped:Connect(function()
	if isFacing and character and character:FindFirstChild("HumanoidRootPart") then
		local nearest = getNearestPlayer()
		if nearest and nearest.Character and nearest.Character:FindFirstChild("HumanoidRootPart") then
			local targetPos = nearest.Character.HumanoidRootPart.Position
			local root = character.HumanoidRootPart
			root.CFrame = CFrame.new(root.Position, Vector3.new(targetPos.X, root.Position.Y, targetPos.Z))

			nameLabel.Text = "Target: " .. nearest.DisplayName
			local userId = nearest.UserId
			pfp.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. userId .. "&width=420&height=420&format=png"
		else
			nameLabel.Text = "Target: NONE"
			pfp.Image = ""
		end
	end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "CHAOS MUSIC [JUMPSTYLE]ðŸ˜–ðŸ“£ðŸ”Š",
   Callback = function()
   -- The function that takes place when the button is pressed
-- LocalScript in StarterPlayerScripts

local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "ChaosMusicGUI"
gui.ResetOnSpawn = false

-- Sound (only you can hear)
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://1839246711"
sound.Name = "ClientSound"
sound.Volume = 3
sound.Looped = true
sound.Parent = workspace

-- Main Frame
local main = Instance.new("Frame", gui)
main.Size = UDim2.new(0, 300, 0, 160)
main.Position = UDim2.new(0.5, -150, 0.5, -80)
main.AnchorPoint = Vector2.new(0.5, 0.5)
main.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
main.BorderSizePixel = 0
main.Active = true
main.Draggable = true

-- RGB Border
local stroke = Instance.new("UIStroke", main)
stroke.Thickness = 3
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

task.spawn(function()
	while true do
		for i = 0, 1, 0.01 do
			stroke.Color = ColorSequence.new{
				ColorSequenceKeypoint.new(0, Color3.fromHSV(i, 1, 1)),
				ColorSequenceKeypoint.new(1, Color3.fromHSV((i + 0.2) % 1, 1, 1))
			}
			task.wait(0.05)
		end
	end
end)

-- Chaos Background
local chaos = Instance.new("UIGradient", main)
chaos.Rotation = 0
chaos.Color = ColorSequence.new{
	ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 0, 255)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 0))
}

game:GetService("RunService").RenderStepped:Connect(function()
	chaos.Rotation = (chaos.Rotation + 1) % 360
end)

-- Play/Stop Button
local playBtn = Instance.new("TextButton", main)
playBtn.Size = UDim2.new(0.8, 0, 0.3, 0)
playBtn.Position = UDim2.new(0.1, 0, 0.4, 0)
playBtn.Text = "Play Music"
playBtn.TextScaled = true
playBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
playBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
playBtn.Font = Enum.Font.GothamBlack
playBtn.BorderSizePixel = 0

-- Close Button
local closeBtn = Instance.new("TextButton", main)
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 5)
closeBtn.Text = "X"
closeBtn.TextColor3 = Color3.fromRGB(255, 0, 0)
closeBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextScaled = true
closeBtn.MouseButton1Click:Connect(function()
	gui.Enabled = false
end)

-- Minimize Button
local miniBtn = Instance.new("TextButton", main)
miniBtn.Size = UDim2.new(0, 30, 0, 30)
miniBtn.Position = UDim2.new(1, -70, 0, 5)
miniBtn.Text = "_"
miniBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
miniBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
miniBtn.Font = Enum.Font.GothamBold
miniBtn.TextScaled = true

local minimized = false
miniBtn.MouseButton1Click:Connect(function()
	minimized = not minimized
	for _, obj in pairs(main:GetChildren()) do
		if obj:IsA("TextButton") and obj ~= closeBtn and obj ~= miniBtn then
			obj.Visible = not minimized
		end
	end
	main.Size = minimized and UDim2.new(0, 300, 0, 40) or UDim2.new(0, 300, 0, 160)
end)

-- Play/Stop Logic
local playing = false
playBtn.MouseButton1Click:Connect(function()
	playing = not playing
	if playing then
		sound:Play()
		playBtn.Text = "Stop Music"
	else
		sound:Stop()
		playBtn.Text = "Play Music"
	end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Live Coordinate ViewerðŸ”´ðŸ‘€",
   Callback = function()
   -- The function that takes place when the button is pressed
local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Name = "CoordinateGUI"
gui.Parent = player:WaitForChild("PlayerGui")
gui.ResetOnSpawn = false

-- Main frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 320, 0, 180)
frame.Position = UDim2.new(0.3, 0, 0.3, 0)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
frame.BorderSizePixel = 3
frame.Active = true
frame.Draggable = true
frame.Parent = gui

-- RGB border animation
task.spawn(function()
	local hue = 0
	while frame.Parent do
		frame.BorderColor3 = Color3.fromHSV(hue, 1, 1)
		hue = (hue + 0.01) % 1
		task.wait(0.03)
	end
end)

-- Math background animation
task.spawn(function()
	while frame.Parent do
		local r = math.abs(math.sin(tick())) * 255
		local g = math.abs(math.sin(tick() + 2)) * 255
		local b = math.abs(math.sin(tick() + 4)) * 255
		frame.BackgroundColor3 = Color3.fromRGB(r, g, b)
		task.wait(0.03)
	end
end)

-- TopBar for controls
local topBar = Instance.new("Frame", frame)
topBar.Size = UDim2.new(1, 0, 0, 30)
topBar.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
topBar.BorderSizePixel = 0

local title = Instance.new("TextLabel", topBar)
title.Size = UDim2.new(1, -60, 1, 0)
title.Position = UDim2.new(0, 10, 0, 0)
title.Text = "Coordinate Viewer"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamSemibold
title.TextSize = 14
title.BackgroundTransparency = 1
title.TextXAlignment = Enum.TextXAlignment.Left

-- Close Button
local closeBtn = Instance.new("TextButton", topBar)
closeBtn.Size = UDim2.new(0, 30, 1, 0)
closeBtn.Position = UDim2.new(1, -30, 0, 0)
closeBtn.Text = "âŒ"
closeBtn.TextColor3 = Color3.new(1, 1, 1)
closeBtn.Font = Enum.Font.Gotham
closeBtn.TextSize = 16
closeBtn.BackgroundColor3 = Color3.fromRGB(80, 0, 0)
Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(0, 6)

-- Minimize Button
local minBtn = Instance.new("TextButton", topBar)
minBtn.Size = UDim2.new(0, 30, 1, 0)
minBtn.Position = UDim2.new(1, -60, 0, 0)
minBtn.Text = "ðŸ”½"
minBtn.TextColor3 = Color3.new(1, 1, 1)
minBtn.Font = Enum.Font.Gotham
minBtn.TextSize = 16
minBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Instance.new("UICorner", minBtn).CornerRadius = UDim.new(0, 6)

-- Coordinate Viewer Box
local coordBox = Instance.new("TextBox", frame)
coordBox.Size = UDim2.new(1, -20, 0, 80)
coordBox.Position = UDim2.new(0, 10, 0, 40)
coordBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
coordBox.TextColor3 = Color3.new(1, 1, 1)
coordBox.TextSize = 16
coordBox.Font = Enum.Font.Gotham
coordBox.TextWrapped = true
coordBox.TextEditable = false
coordBox.ClearTextOnFocus = false
coordBox.Text = "Coordinates: Loading..."
Instance.new("UICorner", coordBox).CornerRadius = UDim.new(0, 6)

-- Copy Button (Smooth style)
local copyBtn = Instance.new("TextButton", frame)
copyBtn.Size = UDim2.new(0.5, -15, 0, 35)
copyBtn.Position = UDim2.new(0, 10, 1, -45)
copyBtn.Text = "ðŸ“‹ Copy"
copyBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
copyBtn.TextColor3 = Color3.new(1, 1, 1)
copyBtn.TextSize = 17
copyBtn.Font = Enum.Font.Gotham
copyBtn.BackgroundTransparency = 0.1
copyBtn.AutoButtonColor = false
Instance.new("UICorner", copyBtn).CornerRadius = UDim.new(0, 8)

local gradient = Instance.new("UIGradient", copyBtn)
gradient.Color = ColorSequence.new{
	ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 100)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 255))
}
gradient.Rotation = 45

-- Hover effect
copyBtn.MouseEnter:Connect(function()
	copyBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
end)
copyBtn.MouseLeave:Connect(function()
	copyBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
end)

-- Coordinate update
task.spawn(function()
	while frame.Parent do
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if hrp then
			local pos = hrp.Position
			coordBox.Text = string.format("Coordinates:\nX: %.1f\nY: %.1f\nZ: %.1f", pos.X, pos.Y, pos.Z)
		end
		task.wait(0.2)
	end
end)

-- Copy button logic
copyBtn.MouseButton1Click:Connect(function()
	local raw = coordBox.Text:gsub("Coordinates:\n", ""):gsub("\n", ", ")
	pcall(function() setclipboard(raw) end)
	copyBtn.Text = "âœ… Copied!"
	task.delay(1, function()
		copyBtn.Text = "ðŸ“‹ Copy"
	end)
end)

-- Minimize / Restore toggle
local minimized = false
minBtn.MouseButton1Click:Connect(function()
	minimized = not minimized
	if minimized then
		coordBox.Visible = false
		copyBtn.Visible = false
		frame.Size = UDim2.new(0, 320, 0, 40)
		minBtn.Text = "ðŸ”¼"
	else
		coordBox.Visible = true
		copyBtn.Visible = true
		frame.Size = UDim2.new(0, 320, 0, 180)
		minBtn.Text = "ðŸ”½"
	end
end)

-- Close button
closeBtn.MouseButton1Click:Connect(function()
	gui:Destroy()
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "PLAYER TRACER GUIã€°ï¸ðŸ‘¤",
   Callback = function()
   -- The function that takes place when the button is pressed
-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

-- Variables
local TracerEnabled = false
local TracerParts = {}

-- GUI Setup
local ScreenGui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
ScreenGui.Name = "TracerGUI"
ScreenGui.ResetOnSpawn = false

local MainFrame = Instance.new("Frame")
MainFrame.Parent = ScreenGui
MainFrame.Size = UDim2.new(0, 300, 0, 180)
MainFrame.Position = UDim2.new(0.05, 0, 0.2, 0)
MainFrame.BackgroundColor3 = Color3.new(1, 1, 1)
MainFrame.BorderSizePixel = 3
MainFrame.Draggable = true
MainFrame.Active = true

-- Smooth RGB border and background
task.spawn(function()
	while true do
		local t = tick() * 2
		local r = 0.5 + 0.5 * math.sin(t)
		local g = 0.5 + 0.5 * math.sin(t + 2)
		local b = 0.5 + 0.5 * math.sin(t + 4)
		MainFrame.BorderColor3 = Color3.new(r, g, b)
		MainFrame.BackgroundColor3 = Color3.new(1 - r/2, 1 - g/2, 1 - b/2)
		task.wait(0.03)
	end
end)

-- Title
local Title = Instance.new("TextLabel", MainFrame)
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundTransparency = 1
Title.Text = "Tracer GUI"
Title.Font = Enum.Font.GothamBold
Title.TextSize = 18
Title.TextColor3 = Color3.new(1, 1, 1)

-- Minimize Button
local MinimizeButton = Instance.new("TextButton", MainFrame)
MinimizeButton.Size = UDim2.new(0, 30, 0, 30)
MinimizeButton.Position = UDim2.new(1, -60, 0, 0)
MinimizeButton.Text = "_"
MinimizeButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
MinimizeButton.TextColor3 = Color3.new(1, 1, 1)
MinimizeButton.Font = Enum.Font.GothamBold
MinimizeButton.TextSize = 18

-- Close Button
local CloseButton = Instance.new("TextButton", MainFrame)
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -30, 0, 0)
CloseButton.Text = "X"
CloseButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
CloseButton.TextColor3 = Color3.new(1, 1, 1)
CloseButton.Font = Enum.Font.GothamBold
CloseButton.TextSize = 18

-- Toggle Button
local Toggle = Instance.new("TextButton", MainFrame)
Toggle.Size = UDim2.new(0, 260, 0, 50)
Toggle.Position = UDim2.new(0, 20, 0, 60)
Toggle.Text = "TRACER: OFF"
Toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Toggle.TextColor3 = Color3.new(1, 1, 1)
Toggle.Font = Enum.Font.GothamBold
Toggle.TextSize = 22

-- Minimize Logic
local minimized = false
MinimizeButton.MouseButton1Click:Connect(function()
	minimized = not minimized
	for _, obj in pairs(MainFrame:GetChildren()) do
		if obj:IsA("TextButton") or obj:IsA("TextLabel") then
			if obj ~= MinimizeButton and obj ~= CloseButton then
				obj.Visible = not minimized
			end
		end
	end
	MainFrame.Size = minimized and UDim2.new(0, 300, 0, 30) or UDim2.new(0, 300, 0, 180)
end)

-- Close Button
CloseButton.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
	for _, info in pairs(TracerParts) do
		if info.Beam then info.Beam:Destroy() end
		if info.Attachment then info.Attachment:Destroy() end
		if info.Billboard then info.Billboard:Destroy() end
	end
end)

-- Toggle Button
Toggle.MouseButton1Click:Connect(function()
	TracerEnabled = not TracerEnabled
	Toggle.Text = TracerEnabled and "TRACER: ON" or "TRACER: OFF"
	Toggle.BackgroundColor3 = TracerEnabled and Color3.fromRGB(30, 130, 30) or Color3.fromRGB(50, 50, 50)

	if not TracerEnabled then
		for _, info in pairs(TracerParts) do
			if info.Beam then info.Beam:Destroy() end
			if info.Attachment then info.Attachment:Destroy() end
			if info.Billboard then info.Billboard:Destroy() end
		end
		TracerParts = {}
	end
end)

-- Tracer Logic
RunService.RenderStepped:Connect(function()
	if not TracerEnabled then return end

	-- Remove old beams
	for _, info in pairs(TracerParts) do
		if info.Beam then info.Beam:Destroy() end
		if info.Attachment then info.Attachment:Destroy() end
		if info.Billboard then info.Billboard:Destroy() end
	end
	TracerParts = {}

	-- Make sure LocalPlayer has a HumanoidRootPart
	if not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then return end
	local localHRP = LocalPlayer.Character.HumanoidRootPart

	local myAttachment = Instance.new("Attachment", localHRP)

	for _, plr in pairs(Players:GetPlayers()) do
		if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
			local targetHRP = plr.Character.HumanoidRootPart

			local targetAttachment = Instance.new("Attachment", targetHRP)

			-- Create beam
			local beam = Instance.new("Beam")
			beam.Attachment0 = myAttachment
			beam.Attachment1 = targetAttachment
			beam.Width0 = 0.1
			beam.Width1 = 0.1
			beam.FaceCamera = true
			beam.LightEmission = 1
			beam.Transparency = NumberSequence.new(0)
			local t = tick() * 2
			local r = 0.5 + 0.5 * math.sin(t)
			local g = 0.5 + 0.5 * math.sin(t + 2)
			local b = 0.5 + 0.5 * math.sin(t + 4)
			beam.Color = ColorSequence.new(Color3.new(r, g, b))
			beam.Parent = localHRP

			-- Billboard for username
			local billboard = Instance.new("BillboardGui", targetHRP)
			billboard.Size = UDim2.new(0, 100, 0, 20)
			billboard.StudsOffset = Vector3.new(0, 3, 0)
			billboard.AlwaysOnTop = true
			local text = Instance.new("TextLabel", billboard)
			text.Size = UDim2.new(1, 0, 1, 0)
			text.BackgroundTransparency = 1
			text.Text = plr.DisplayName
			text.TextColor3 = Color3.new(1, 1, 1)
			text.Font = Enum.Font.GothamBold
			text.TextScaled = true

			table.insert(TracerParts, {
				Beam = beam,
				Attachment = targetAttachment,
				Billboard = billboard
			})
		end
	end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "COORDINATE TELEPORTERâž•ï¸âž–ï¸ðŸ‘¤",
   Callback = function()
   -- The function that takes place when the button is pressed
-- GUI Setup
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local teleportGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
teleportGui.Name = "TeleportGUI"
