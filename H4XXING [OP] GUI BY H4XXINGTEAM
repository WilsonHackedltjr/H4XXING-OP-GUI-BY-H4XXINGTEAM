local Button = Tab:CreateButton({
   Name = "NEON GLOW CHARACTERðŸ”³ðŸ‘¤",
   Callback = function()
   -- The function that takes place when the button is pressed
-- Server-side script: Place in ServerScriptService

local Players = game:GetService("Players")

-- Adjust this to control neon glow intensity
local NEON_MATERIAL = Enum.Material.Neon
local GLOW_COLOR = Color3.new(0.8, 0.8, 1) -- pale blue glow

local function applyNeon(character)
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Material = NEON_MATERIAL
            part.Color = GLOW_COLOR
        end
    end
end

local function onPlayer(player)
    player.CharacterAdded:Connect(function(char)
        char:WaitForChild("HumanoidRootPart") -- ensure character is ready
        applyNeon(char)
    end)
    -- If character already exists (live mode), apply immediately:
    if player.Character then
        applyNeon(player.Character)
    end
end

-- Apply to each player (existing + new)
for _, plr in pairs(Players:GetPlayers()) do
    onPlayer(plr)
end
Players.PlayerAdded:Connect(onPlayer)
   end,
})


local Button = Tab:CreateButton({
   Name = "Undo NEON GLOW CHARACTERðŸ”³ðŸ‘¤",
   Callback = function()
   -- The function that takes place when the button is pressed
-- ðŸ§¯ Undo Neon Glow (Character Reset Script)
-- Put this in ServerScriptService

local Players = game:GetService("Players")

-- Replace this with your actual Roblox UserId
local YOUR_USER_ID = 8317715190  -- â† Change this to your UserId

local function removeNeonGlow(character)
	for _, part in pairs(character:GetDescendants()) do
		if part:IsA("BasePart") then
			if part.Material == Enum.Material.Neon then
				part.Material = Enum.Material.Plastic
				part.Color = Color3.new(0.8, 0.8, 0.8) -- light gray or original color
			end
		end
	end

	-- Remove outline/Highlight if present
	for _, obj in pairs(character:GetChildren()) do
		if obj:IsA("Highlight") then
			obj:Destroy()
		end
	end
end

-- Listen for the correct player's character
Players.PlayerAdded:Connect(function(player)
	if player.UserId == YOUR_USER_ID then
		player.CharacterAdded:Connect(function(character)
			wait(1)
			removeNeonGlow(character)
		end)
	end
end)

-- Instant cleanup if already in game
for _, player in ipairs(Players:GetPlayers()) do
	if player.UserId == YOUR_USER_ID and player.Character then
		removeNeonGlow(player.Character)
	end
end
   end,
})

